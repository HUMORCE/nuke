{
    "version": "4.3.0",
    "description": "The leading WebAssembly Runtime supporting WASI and Emscripten",
    "homepage": "https://wasmer.io/",
    "license": "MIT",
    "suggest": {
        "wapm": "nuke/wapm-cli"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/wasmerio/wasmer/releases/download/v4.3.0/wasmer-windows-amd64.tar.gz",
