{
    "version": "3.1.0",
    "description": "The leading WebAssembly Runtime supporting WASI and Emscripten.",
    "homepage": "https://wasmer.io/",
    "license": "MIT",
    "suggest": {
        "wapm": "nuke/wapm-cli"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/wasmerio/wasmer/releases/download/v3.1.0/wasmer-windows-amd64.tar.gz",
            "hash": "6f49be0023571f9d4edab36c4acdd449c3a03da483c1f4412fbfcc98fe455dce"
        }
    },
    "env_set": {
        "WASMER_DIR": "$dir",
        "WASMER_CACHE_DIR": "$dir\\cache"
    },
    "bin": "bin\\wasmer.exe",
    "persist": "globals",
    "checkver": {
        "github": "https://github.com/wasmerio/wasmer"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/wasmerio/wasmer/releases/download/v$version/wasmer-windows-amd64.tar.gz"
            }
        }
    }
}
